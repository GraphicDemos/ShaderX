   <RmEffectWorkspace NAME="Effect Workspace" TYPE="RmEffectWorkspace" API="None" HIDDEN="FALSE" GUID="1b2cdf45-1446-42e7-8a2b-165c9ed6db55" ACTIVE_EFFECT="Dot3 Cel Shading with Dynamic Light Source" VALIDATION_STRING="" XML_VERSION="RenderMonkey 1.0">
