      <RmEffectGroup NAME="Cel Shading" TYPE="RmEffectGroup" API="None" HIDDEN="FALSE" GUID="b4026ee2-5e0a-430f-b371-741afb7e9770">
         <RmDirectXEffect NAME="Dot3 Cel Shading with Dynamic Light Source" TYPE="RmDirectXEffect" API="D3D" HIDDEN="FALSE" GUID="57931328-e9bd-4512-9c67-97250db38daf" ACTIVE_CAMERA="">
            <RmStringVariable NAME="Dot3CelShadingEffectNotes" TYPE="RmStringVariable" API="None" HIDDEN="FALSE" GUID="9c9534fc-7e4b-4de7-a8a2-3ffc29a6f8d3" ARTIST_EDITABLE="FALSE" VALUE="Programmer:  Ronald Barbosa **rbarbosa@exibeo.net**&#xA;&#xA;Purpose:  To employ Dot3 Cel Shading in a way that separates&#xA;the light source from the camera.  This implementation provides&#xA;an animated light source that emits white diffuse and specular&#xA;lighting.  The specular component is calculated per pixel and&#xA;creates and interesting combination of realistic lighting with&#xA;unrealistic rendering.&#xA;"/>
            <RmFloatVariable NAME="edge_width" TYPE="RmFloatVariable" API="None" HIDDEN="FALSE" GUID="98dcaf33-fd3b-4546-a099-203fff79b579" ARTIST_EDITABLE="TRUE" VALUE="0.350000" MIN="0.000000" MAX="1.000000" CLAMP="FALSE">
               <RmStringVariable NAME="edge_widthNotes" TYPE="RmStringVariable" API="None" HIDDEN="FALSE" GUID="0c400e4d-35c1-494a-a20a-77a6f2f842ed" ARTIST_EDITABLE="FALSE" VALUE="This variable is used to set the width of the hard edge&#xA;of the cel shaded model.  Since the light source has&#xA;been separated from the camera, the approach used&#xA;here is to define a threshold value (edge_width).  Pixels&#xA;where the normal vector and the viewing vector form an&#xA;angle whose cosine is less than this threshold are&#xA;drawn using the edge color.&#xA;&#xA;A setting of 0.0f will eliminate the hard edge, and a&#xA;setting of 1.0f will create a model rendered entirely with&#xA;the edge color."/>
            </RmFloatVariable>
            <RmFloatVariable NAME="shades" TYPE="RmFloatVariable" API="None" HIDDEN="FALSE" GUID="434def87-0b0f-44d2-9589-a09eee94d2b2" ARTIST_EDITABLE="TRUE" VALUE="2.000000" MIN="0.000000" MAX="100.000000" CLAMP="FALSE">
               <RmStringVariable NAME="shadesNotes" TYPE="RmStringVariable" API="None" HIDDEN="FALSE" GUID="66d257a6-9b49-48e4-a3ac-26f032a4bfe1" ARTIST_EDITABLE="FALSE" VALUE="This variable represents the total number of color shades&#xA;that will be used when rendering the image.  This setting&#xA;only applies to the number of shades created by the light&#xA;source.  Additional shades will be created by the hard edge&#xA;as well as the ambient light level.&#xA;&#xA;Increasing this value will add more visible color shades.&#xA;&#xA;This variable should always be an integer for best results, and&#xA;it must never be set to zero."/>
            </RmFloatVariable>
            <RmFloatVariable NAME="ambient_level" TYPE="RmFloatVariable" API="None" HIDDEN="FALSE" GUID="8b3fd73f-2341-4bf1-af9b-2f588bbf0c29" ARTIST_EDITABLE="TRUE" VALUE="0.200000" MIN="-1.000000" MAX="1.000000" CLAMP="FALSE">
               <RmStringVariable NAME="ambient_levelNotes" TYPE="RmStringVariable" API="None" HIDDEN="FALSE" GUID="e6ca9825-b848-46a6-8ee6-aadf4a8e3081" ARTIST_EDITABLE="FALSE" VALUE="The ambient light level that will be used for rendering&#xA;the unlit vertices of the mesh."/>
            </RmFloatVariable>
            <RmFloatVariable NAME="cos_time_0_X" TYPE="RmFloatVariable" API="None" HIDDEN="FALSE" GUID="dc848674-96da-43e7-9c43-df49d26cf00a" ARTIST_EDITABLE="FALSE" VALUE="-0.045702" MIN="-1.000000" MAX="1.000000" CLAMP="FALSE">
               <RmStringVariable NAME="cos_time_0_XNotes" TYPE="RmStringVariable" API="None" HIDDEN="FALSE" GUID="7bb87c33-e786-4b4c-be03-6f4ecc6414db" ARTIST_EDITABLE="FALSE" VALUE="Variable used for rotating the light source."/>
            </RmFloatVariable>
            <RmFloatVariable NAME="sin_time_0_X" TYPE="RmFloatVariable" API="None" HIDDEN="FALSE" GUID="77deccbd-e60e-44bd-848e-2efb0250cb06" ARTIST_EDITABLE="FALSE" VALUE="0.998955" MIN="-1.000000" MAX="1.000000" CLAMP="FALSE">
               <RmStringVariable NAME="sin_time_0_XNotes" TYPE="RmStringVariable" API="None" HIDDEN="FALSE" GUID="914940d0-abd0-4155-bfcc-1c541308a6b0" ARTIST_EDITABLE="FALSE" VALUE="Variable used for rotating the light source."/>
            </RmFloatVariable>
            <RmFloatVariable NAME="shininess" TYPE="RmFloatVariable" API="None" HIDDEN="FALSE" GUID="ab320a01-59b2-47e6-80c5-55144a3feed8" ARTIST_EDITABLE="TRUE" VALUE="20.000000" MIN="0.000000" MAX="100.000000" CLAMP="FALSE">
               <RmStringVariable NAME="shininessNotes" TYPE="RmStringVariable" API="None" HIDDEN="FALSE" GUID="e2bd6099-db12-4e01-83c0-223e5fcd0671" ARTIST_EDITABLE="FALSE" VALUE="This is the &quot;power&quot; of the specular highlight.  Increasing this&#xA;value creates a smaller, more focused highlight.  Decreasing&#xA;this value wil result in a more scattered highlight."/>
