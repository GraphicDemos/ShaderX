mov r0, c0
dp3 r1, r0, c1
dp3 r0, r1, r0
dp3 r1, r0, r1
dp3 r0, r1, r0
dp3 r1, r0, r1
dp3 r0, r1, r0
dp3 r1, r0, r1
dp3 r0, r1, r0
dp3 r1, r0, r1
dp3 r0, r1, r0
dp3 r1, r0, r1
dp3 r0, r1, r0
dp3 r1, r0, r1
dp3 r0, r1, r0
dp3 r1, r0, r1
dp3 r0, r1, r0


// ---------------------------------------------------------------------------------------
// Output color
mov oC0, r0

