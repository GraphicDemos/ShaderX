//----------------------------------------------------------------------------------------
// Pixel shader for rendering shadowed scene
// ---------------------------------------------------------------------------------------

ps.2.0

// Declare incoming registers
dcl t0.xy				// coordinates	

def c30, -1.5500992e-006f, -2.1701389e-005f,  0.0026041667f, 0.00026041668f
def c31, -0.020833334f, -0.12500000f, 1.0f, 0.50000000f 
mov r0, c0

sincos_pp r1.x, r0.x, c30, c31
mov r1.yzw, c0
// ---------------------------------------------------------------------------------------
// Output color
mov oC0, r1

